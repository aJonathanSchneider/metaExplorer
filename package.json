{
  "author": "Jonathan Schneider",
  "name": "bizsys-editor",
  "version": "0.0.1",
  "description": "a GUI editor for configuring bizSys-Server",
  "license": "MIT",
  "main": "src/index.js",
  "private": true,
  "jest": {
    "verbose": false,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig-jest.json"
      }
    }
  },
  "scripts": {
    "test": "jest --watch",
    "build": "webpack --config=webpack.prod.config.js",
    "start": "cross-env NODE_ENV=development METAEXPLORER_MODE=demo webpack-dev-server",
    "startfornetwork": "webpack-dev-server --host 0.0.0.0",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "compcss": "node-sass --output-style compressed ./src/styles/styles.scss > cat ./dist/style.css",
    "createmock": "node ./scripts/createItptMock.js",
    "dl-tooling": "node ./scripts/dlTooling.js",
    "generateGoogleSheetsClient": "java -jar ./tooling/swagger-codegen-cli.jar generate -i https://raw.githubusercontent.com/APIs-guru/openapi-directory/master/APIs/googleapis.com/sheets/v4/swagger.yaml -l typescript-fetch -o ./src/google/apis/sheets-generated"
  },
  "dependencies": {
    "@material-ui/core": "^4.0.1",
    "@material-ui/icons": "^4.0.1",
    "@material-ui/pickers": "^3.0.0",
    "@types/dagre": "^0.7.40",
    "@types/gapi": "^0.0.36",
    "@types/gapi.auth2": "^0.0.48",
    "@types/gapi.client.sheets": "4.0.0",
    "@types/jasmine": "^3.3.13",
    "assert-never": "^1.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "dagre": "^0.8.2",
    "googleapis": "^32.0.0",
    "jasmine": "^3.4.0",
    "keycloak-js": "^6.0.0",
    "lodash": "^4.17.4",
    "metaexplorer-react-components": "file:../metaExplorer-react-components",
    "qr-scanner": "^1.1.1",
    "qrcode-generator": "^1.4.3",
    "quagga": "^0.12.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-dropzone": "^7.0.1",
    "react-gooey-nav": "^2.0.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.5.2",
    "storm-react-diagrams": "^5.2.1",
    "swagger-client": "^3.8.24"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@types/classnames": "^2.2.6",
    "@types/jest": "^24.0.14",
    "@types/lodash": "^4.14.118",
    "@types/node": "11.9.4",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.1",
    "@types/react-dropzone": "^4.2.2",
    "@types/react-redux": "^6.0.9",
    "@types/react-router": "^4.4.4",
    "@types/react-router-dom": "^4.3.1",
    "@types/rx": "^4.1.1",
    "babel-jest": "^23.6.0",
    "babel-preset-minify": "^0.5.0",
    "classnames": "^2.2.6",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^3.1.0",
    "postcss-each": "^0.10.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.0",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^22.4.2",
    "ts-loader": "^6.0.2",
    "tslint": "^5.17.0",
    "typeface-roboto": "^0.0.54",
    "typescript": "3.5.2",
    "url-loader": "^0.6.2",
    "webpack": "^4.34.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "write-file-webpack-plugin": "^4.5.0"
  }
}
