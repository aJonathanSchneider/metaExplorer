{
  "repository": {
    "type": "git",
    "url": "https://https://github.com/aJonathanSchneider/metaExplorer.git"
  },
  "devDependencies": {
    "lerna": "^3.22.1"
  },
  "workspaces": [
    "packages/*",
    "mods/*",
    "nocode/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "tsc": "lerna run tsc",
    "prebuild": "lerna run buildcore",
    "build": "lerna run build",
    "dev": "set MIO_DYNAMIC=true && lerna run start --stream",
    "start": "set OVERWRITE_ON_RESTART=true && yarn dev",
    "release": "npm config set access public && yarn lerna publish"
  },
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "license": "EUPL-1.2"
}