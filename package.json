{
  "author": "Jonathan Schneider",
  "name": "bizsys-editor",
  "version": "0.0.1",
  "description": "a GUI editor for configuring bizSys-Server",
  "license": "MIT",
  "main": "src/index.js",
  "private": true,
  "jest": {
    "verbose": false,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig-jest.json"
      }
    }
  },
  "scripts": {
    "test": "jest --watch",
    "build": "webpack -p --config=webpack.prod.config.js",
    "start": "webpack-dev-server",
    "startfornetwork": "webpack-dev-server --host 0.0.0.0",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "compcss": "node-sass --output-style compressed ./src/styles/styles.scss > cat ./dist/style.css"
  },
  "dependencies": {
    "assert-never": "^1.0.0",
    "hydraclient.js": "https://github.com/HydraCG/Heracles.ts.git",
