    "hydraclient.js": "https://github.com/HydraCG/Heracles.ts.git",
    "lodash": "^4.17.4",
    "m-react-splitters": "^1.0.4",
    "metaexplorer-react-components": "file:../metaExplorer-react-components",
    "quagga": "^0.12.1",
    "react": "^16.6.1",
    "react-accessible-accordion": "^2.4.5",
    "react-collapsible": "^2.3.1",
    "react-dom": "^16.6.1",
