    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "caniuse-lite": "^1.0.30000760",
